  "scripts": {
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint:js": "env-cmd -e pre:lint -r ./env-cmdrc.js eslint \"src/**/*.{js,jsx}\" --quiet",
    "setup:dev": "yarn && yarn build-dll:dev",
    "setup:prod": "yarn && yarn build-dll:prod",
    "build-dll:dev": "webpack --config build/webpack_dll.config.js --mode development --devtool eval-source-map",
    "build-dll:prod": "webpack --config build/webpack_dll.config.js --mode production --devtool nosources-source-map",
    "dev": "webpack-dev-server --progress --colors --config build/webpack.dev.js",
    "start": "yarn setup:dev && env-cmd -e start:dev -r ./env-cmdrc.js yarn dev",
    "start-b": "yarn setup:dev && BUNDLE=1 yarn dev",
    "release": "webpack --progress --colors --config build/webpack.prod.js --profile --json > stats.json",
    "build-s": "yarn setup:prod && env-cmd -e build:staging -r ./env-cmdrc.js yarn release",
    "build-p": "yarn setup:prod && env-cmd -e build:prod -r ./env-cmdrc.js yarn release",
    "ws": "ws -d www/main"
  },
